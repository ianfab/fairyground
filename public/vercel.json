{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" },
        { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" }
      ]
    },
    {
      "source": "/lib/stockfish.wasm",
      "headers": [
        { "key": "x-decompressed-content-length", "value": "1463349" }
      ]
    },
    {
      "source": "/*.jpg",
      "headers": [
        { "key": "cache-control", "value": "max-age=7200,must-revalidate" }
      ]
    }
  ]
}
