{
  "headers": [
     {
        "source": "/(.*)",
        "headers": [
        {
           "key": "Cross-Origin-Embedder-Policy",
           "value": "require-corp"
        },
        {
           "key": "Cross-Origin-Opener-Policy",
           "value": "same-origin"
        }
        ]
     },
     {
        "source": "/(.*).(avif|svg)",
        "headers": [
        { 
           "key": "Cache-Control",
           "value": "public,max-age=3600,must-revalidate,stale-while-revalidate=2419200"
        },
        { 
           "key": "CDN-Cache-Control",
           "value": "public,max-age=10,stale-while-revalidate=590"
        },
        { 
           "key": "Vercel-CDN-Cache-Control",
           "value": "public,max-age=10,stale-while-revalidate=590"
        }
        ]
     },
     {
        "source": "/(.*).(woff2|woff|ttf)",
        "headers": [
        { 
           "key": "Cache-Control",
           "value": "public,max-age=86400,must-revalidate,stale-while-revalidate=2419200"
        },
        { 
           "key": "CDN-Cache-Control",
           "value": "public,max-age=10,stale-while-revalidate=590"
        },
        { 
           "key": "Vercel-CDN-Cache-Control",
           "value": "public,max-age=10,stale-while-revalidate=590"
        }
        ]
     }
  ]
}