{
  "headers": [
     {
        "source": "/(.*)",
        "headers": [
        {
           "key": "Cross-Origin-Embedder-Policy",
           "value": "require-corp"
        },
        {
           "key": "Cross-Origin-Opener-Policy",
           "value": "same-origin"
        }
        ]
     },
     {
        "source": "/(.*).avif",
        "headers": [
        { 
           "key": "Cache-Control",
           "value": "public,max-age=86400,must-revalidate"
        }
        ]
     }
  ]
}