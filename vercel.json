{
  "headers": [
     {
        "source": "/(.*)",
        "headers": [
        {
           "key": "Cross-Origin-Embedder-Policy",
           "value": "require-corp"
        },
        {
           "key": "Cross-Origin-Opener-Policy",
           "value": "same-origin"
        }
        ]
     },
     {
        "source": "/*.jpg",
        "headers": [
        { 
           "key": "cache-control",
           "value": "max-age=7200,must-revalidate"
        }
        ]
     }
  ]
}